<h1 align="center">Listing All Categories</h1>
<div align="center">
  <%= will_paginate %>
  <% @categories.each do |category| %>
    <div class="card-header">
      <div class="row">
        <div class="col">
            <%= link_to "#{category.name}", category_path(category) %>
        </div>
      </div>
    </div>
<% end %>
  <%= will_paginate %>
</div>